            </nav>
        </div>
    </div>
    <div class="col"></div>

</div>

<script>
    $content = $("#content");


    function renderRows(data) {
        var rows;

        data.forEach(function(item) {
            rows += `
<tr>
    <td>${item}</td>
    <td></td>
</tr>
`;
        });

        return rows;
    }

    function renderPagination() {
        var pagination;
        return pagination;
    }

    $content.append(renderRows([1, 2, 3]));


</script>