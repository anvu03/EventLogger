<div class="row">
    <div class="col">
        <h5>Login Count Per App</h5>
        <table class="table table-bordered table-striped">
            <thead>
            <tr>
                <th>App</th>
                <th>Count</th>
            </tr>
            </thead>
            <tbody id="content">
            </tbody>
        </table>
        <div>
            <nav aria-label="Page navigation example">
                <ul class="pagination">
                    <li class="page-item">
                        <a class="page-link" href="#">Previous</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="#">1</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" href="#">2</a>
                    </li>
                    <li class="page-item">
